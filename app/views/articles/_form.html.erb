<%= form_for @article do |form| %>
  <p>
    <%= form.label :title %><br>
    <%= form.text_field :title %>
  </p>

  <p>
    <%= form.label :descrpition %><br>
    <%= form.text_area :description %>
  </p>
  <p>
    <%= form.label :region %><br>
    <%= form.text_area :region %>
  </p>
  <p>
    <%= form.label "Audio File" %><br>
    <%= form.file_field :song_file, multiple: true, direct_upload: true %>
  </p>
  <p>
    <%= form.submit %>
  </p>
<% end %>
